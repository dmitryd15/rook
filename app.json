{
  "expo": {
    "name": "Auxilliator",
    "slug": "Rook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aggreyo.Rook"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
    "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e35f90b2-e908-4c1b-a0d9-a89e13ba7b47"
      }
    },
    "splash": {
      "image": "./assets/images/icon.png"
    },
    "owner": "aggrey-o",
    "notification": {
      "vapidPublicKey": "BLzwnz6RqSgmA9XLA2uqCZQ7wZApQgjEyRNRbg7dYoxk-i7uHIGnLGUyrhPqOVks8eejPviXuwnoFwkWlHgnMPI",
      "serviceWorkerPath": "service-worker.js"
    }
  }
}
